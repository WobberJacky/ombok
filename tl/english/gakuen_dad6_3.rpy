# game/gakuen_dad6_3.rpy:932
translate english gakuen_dad6_3c_f65b4524:

    # T "（…………）"
    T ""

# game/gakuen_dad6_3.rpy:938
translate english gakuen_dad6_3c_8cc09734:

    # T "「ま、まさか自力で……？」"
    T ""

# game/gakuen_dad6_3.rpy:947
translate english gakuen_dad6_3c_808ba749:

    # voice dad_g0376
    # Dee "「うん、壁なんて邪魔なだけだからね。\n 僕と兄弟で壊しちゃったんだよ」"
